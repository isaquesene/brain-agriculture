generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Users {
  userId  String @id
  name    String
  email   String
}

model Produtores {
  produtorId      String   @id
  cpf             String?  @unique
  cnpj            String?  @unique
  nome            String
  nomeFazenda     String
  cidade          String
  estado          String
  areaTotal       Float
  areaAgricultavel Float
  areaVegetacao   Float
  culturas        String[]
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
